{
    "manifest_version": 3,
    "name": "Network Request Counter",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "storage"
    ],
    "host_permissions": [
    "<all_urls>"
  ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  